<script type="text/javascript">
function fun(allNum,num){
	for(var i=1;i<=allNum;i++){
		$("#div-"+i).hide();
	}
	$("#div-"+num).show();
}
</script>
<!-- 点击按钮颜色转换JS启动 -->
<script type="text/javascript">
    $(function(){
        $('.on1').click(function(){
            $('.on1').addClass('active');
            $('.on2').removeClass('active');
			$('.on3').removeClass('active');
			$('.on4').removeClass('active');
        });
        $('.on2').click(function(){
            $('.on2').addClass('active');
            $('.on1').removeClass('active');
			$('.on3').removeClass('active');
			$('.on4').removeClass('active');
        });
		$('.on3').click(function(){
            $('.on3').addClass('active');
            $('.on1').removeClass('active');
			$('.on2').removeClass('active');
			$('.on4').removeClass('active');
        });
        $('.on4').click(function(){
            $('.on4').addClass('active');
            $('.on1').removeClass('active');
			$('.on2').removeClass('active');
			$('.on3').removeClass('active');
        });
	});
 </script>
