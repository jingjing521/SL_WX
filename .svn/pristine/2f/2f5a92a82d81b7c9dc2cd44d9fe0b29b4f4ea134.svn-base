	<script>
		$(document).ready(function(){
			var bh=$(".center-top-bg").height();
			$(".center-top").css("min-height","241px");
			$(".center-top").css("height",bh);
			//ajax
			 var user_id=localStorage.getItem("user_id");
			 var user_id1=parseInt(user_id);
			 localStorage.setItem('user_id',user_id1);
			 var timestamp1 = Date.parse(new Date());
 			 var values1=["user_id","timestamp"];
			 var data1={"user_id":user_id1,"timestamp":timestamp1};
			 var sign1 = doSign(values1,data1);
			 var keys="idf5nsi5t0qbemwo12hztbftm53tbv6pht";
			 $.ajax({
				type:"post",
				url: link_url+"getSelfInfos",
				dataType: "json",
			    data: {"user_id":user_id1,"key":keys,"timestamp":timestamp1,"sign":sign1},
				success: function(data) {
				    if(data.status == 1){
				        console.log(data);
				        var imgsrc=data.data.avatar;
				        if(data.data.avatar==""){
				        	imgsrc="images/3.png";
				        }else{
				        	imgsrc=data.data.avatar;
				        }
				        if(data.data.is_company==0){
				        	console.log(1111);
				        	console.log(data.data.id);
							$('<a href="set.html?id='+data.data.id+'"><img src="images/center-top (3).png" class="center-top-set" /></a><img src="images/cen.png" class="center-top-border" /><a href="personal-data.html"><img src="'+imgsrc+'" class="center-top-tx" /></a><div class="center-top-name">'+data.data.user_nicename+'</div>').appendTo(".center-top-po");
				        }else if(data.data.is_company==1){
				        	$('<a href="set.html?id='+data.data.id+'"><img src="images/center-top (3).png" class="center-top-set" /></a><img src="images/center-top.png" class="center-top-border" /><a href="personal-data.html"><img src="'+imgsrc+'" class="center-top-tx" /></a><img src="images/center-top (4).png" class="center-top-sj"><div class="center-top-name">'+data.data.user_nicename+'</div>').appendTo(".center-top-po");
				        }
				        $(".sprz").click(function(){
				        	if(data.data.auth_status==0){
				        		$.toast("审核中，请等待!","text");
				        	}else if(data.data.auth_status==1){
				        		window.location.href="merchants-pass.html";
				        	}else if(data.data.auth_status==2){
				        		window.location.href="merchants-sb.html";
				        	}else{
//				        		window.location.href="http://qcap.hangtuosoft.com/wx/merchants-certification.html";
								window.location.href="merchants-certification.html";
				        	}
				        })
				        $(".jf").text(data.data.score);
				    }else if(data.status == "-9004") {
				        $('<a href="set.html"><img src="images/center-top (3).png" class="center-top-set" /></a><img src="images/cen.png" class="center-top-border" /><a href="personal-data.html"><img src="images/center-top (2).png" class="center-top-tx" /></a><div class="center-top-name">请登录</div>').appendTo(".center-top-po");
						$.toast("请您先登录~", "text");
						setInterval(function(){
							window.location.href="login.html";
						},1500)
					}else{
				    	console.log(data)
				       $.toast(data.data, "text");
				       $('<a href="set.html"><img src="images/center-top (3).png" class="center-top-set" /></a><img src="images/cen.png" class="center-top-border" /><a href="personal-data.html"><img src="images/center-top (2).png" class="center-top-tx" /></a><div class="center-top-name">请登录</div>').appendTo(".center-top-po");
				    }
				},
				error:function(){
				   
				}
			 })
			
		})
