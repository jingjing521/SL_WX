/*头部*/

/*登录*/
.login-nav{width: 100%; position: relative; height: 100%; background:url(../images/login-bg.png) no-repeat; background-size: 100% 100%; min-height: 200px;}
.login-return{width: 28px; position: absolute; top: 20px; left: 10px;}
.login-logo{width: 80px; height: auto; position: absolute; top: 50%; left: 50%; margin-left:-40px ; margin-top: -26px;}
.login-return img,.login-logo img{width: 100%; height: auto;}
.login-nav-name{width: 100%; height: auto; line-height: 30px; color: #fff; text-align: center; position: absolute; bottom: 0;}
.login-nav-name div{width: 50%; text-align: center; float: left;}
.login-nav-active{background: url(../images/icontop.png) no-repeat 50% 25px;}
.login-main{width: 90%; margin: 30px auto 0;}
.login-input{width: 100%; height: 50px; line-height: 50px; margin-top: 15px; border: 1px solid #ccc; border-radius: 35px;}
.login-img{width: 22px; margin-left: 20px; margin-right: 10px; margin-top: -2px;}
.border-none{width: 80%; line-height: 30px; border: 0;}
.color-cys{ color: #e43494; margin: 10px 0 0;}
.login-inputs{width: 100%; height: 50px; background: #e43494; line-height: 50px; margin-top: 15px; border: 1px solid #e43494; border-radius: 35px;}
.login-inputs input{width: 90%; margin-left: 5%; background: #e43494; color: #fff; font-size: 18px;}
.login-tishi{width: 100%;border-bottom: 1px solid #eee; margin-top:80px;}
.login-text{background: #fff; color: #ddd; width: 100px; text-align: center; margin: auto;margin-top: -10px; }
.lofin-fs{width: 100%;}
.lofin-fs div{width: 33.33%; margin: auto; text-align: center; margin-top: 20px;}
.lofin-fs div img{width: 70px;}

/*忘记密码*/
.reset-logo{width: 100%; height: auto; position: absolute; top: 50%; left: 50%; margin-top: -34px; margin-left: -115px; color: #fff; line-height: 30px;}
.reset-logo-img,.reset-logo-img img{width: 50px;}
.reset-logo-imgjt{margin: 10px 30px 0;}
.reset-logo-imgjt,.reset-logo-imgjt img{width: 60px;}
.reset-logo-imgs{width: 50px; border: 1px solid #fff; border-radius: 50%; height: 50px; line-height: 45px;}
.reset-logo-imgs img{width: 15px;}
.reset-return-title{ position: absolute; top: 20px; left: 50%; color: #fff; line-height: 30px; margin-left: -30px;}
.reset-input{width: 60%; height: 50px; line-height: 50px; margin-top: 15px; border: 1px solid #ccc; border-radius: 35px;}
.reset-input input{width: 65% !important;}
.reset-hq{ width: 37%; margin-left: 2%; margin-top: 15px;}
.reset-hq img{width: 100%; height: 50px;}

/*注册*/
.register-text{ color: #666; line-height: 30px; text-align: center;}
input[type="radio"] {
  margin: 3px 3px 0px 5px;
  display: none;
}
label {
  padding-left: 25px !important;
  cursor: pointer;
  background:url(../images/radios.png) no-repeat 5px;
}
label.checked {
   background:url(../images/radio.png) no-repeat 5px;
}

/*首页*/
#index{width: 100%; height: auto;}
#index .index-nav{width: 100%; height: 50px; line-height: 50px; background: #fff;}
#index .swiper-container {
        width: 100%;
        height: 99%;
    }
#index .swiper-slide {
        text-align: center;
        font-size: 18px;
        background: #fff;
        width: auto;

        /* Center slide text vertically */
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
    }
#index .swiper-containers {
        width: 100%;
        height: auto;
        position: relative;
    }
#index .swiper-slide img{width: 100%;}
#index .swiper-slide:nth-child(1) {
         margin-left: 25px;
    }
#index .swiper-slide-w{ text-align: center; margin-left: 25px; margin-right: 8px;}
#index .swiper-slide-ws{width: 55px; text-align: center;}
#index .swiper-slide a{color: #333;font-size: 16px;}
#index .swiper-active a{ width: 100%; color: #E43494; font-size: 16px; border-bottom: 3px solid #E43494; height: 47px; line-height: 51px;}
#index .index-banner{width: 100%; height: auto; margin-bottom: 5px;}
#index .index-con{width: 100%; height: auto; background: #fff; margin-bottom:10px;}
#index .index-con-title{width: 100%; text-align: center; padding: 15px 0; position: relative; }
#index .index-con-title img{width: 185px;}
#index .index-con-title span{ width: 100%; text-align: center; position: absolute; font-weight: 600; top: 0px; line-height: 55px; font-size: 20px; left: -1px;}
#index .index-con-banner{width: 100%; height: auto;}
#index .index-con-banner img{width: 100%;}
#index .index-con-text{width: 90%; padding: 0 5%; line-height: 50px; color: #333;}
#index .index-con-text img{width: 13px; margin-right: 5px;}
#index .index-con-name{width: 60%; overflow:hidden;text-overflow:ellipsis; -o-text-overflow:ellipsis;white-space:nowrap;}
#index .index-top{width: 90%; overflow: hidden; background: #fff; position: relative; padding: 0 5%; height: 50px; border-bottom: 1px solid #ddd; line-height: 50px;}
#index .index-top-img{ width: 25px;}
#index .index-top-img img{width: 25px;}
#index .index-top-logo{width: 80px; position: absolute; top: 50%; left: 50%; margin-left: -40px; margin-top: -25px;}
#index .index-top-logo img{ width: 100%;}
/*预售*/
#index .index-item{width: 90%; padding: 5%; border-top: 1px solid #ddd;}
#index .index-item-img{width: 30%; height: auto; text-align: center;}
#index .index-item-img img{width: 100%;}
#index .index-item-r{width: 65%; margin-left: 5%;}
#index .index-item-title{font-size: 18px; color: #333; line-height: 40px;}
#index .index-item-price{font-size: 14px; color: #E43494;}
#index .index-item-price span{font-size: 18px; font-weight: 600;}
#index .index-item-prices{font-size: 13px; color: #B2B2B2; line-height: 0; margin-top: 5px;}
#index .index-item-date{font-size: 14px; color: #666; margin-top: 20px; }
/*公告*/
#index .index-items{width: 90%; padding:3% 5% 3%; border-top: 1px solid #ddd;}
#index .index-item-top{width: 100%; height: auto; color: #333; overflow:hidden;text-overflow:ellipsis; -o-text-overflow:ellipsis;white-space:nowrap;}
#index .index-item-top img{width: 8px; margin-right: 5px;}
#index .index-item-bottom{width: 100%; text-align: right; color: #B2B2B2;}
#index .index-item-bottom img{width: 13px; margin-right: 5px;}
/*消息*/
#information{width: 100%; height: auto;}
#information .information-item{width: 90%; padding:3% 5%; height: auto; background: #fff; border-bottom: 1px solid #ddd; }
#information .information-item:nth-last-child{ border-bottom: 0;}
#information .information-item-img{width: 20%; height: auto; position: relative;}
#information .information-item-img img{width: 100%; border-radius: 50%;}
#information .information-num{color: #fff; text-align: center; !important; position: absolute; right: -1px; top: 1px;}
#information .information-num span{ margin-left: -10px; font-size: 11px; text-align: center; width: 15px;}
#information .information-num img{ width: 15px}
#information .information-item-r{ width: 77%; margin-left: 3%;}
#information .information-item-name{width: 100%; font-size: 18px; color: #333; margin-top: 3px;}
#information .information-item-title{width: 100%; color: #B2B2B2; margin-top: 12px;}

/*搜索分类*/
#search{width: 100%; height: auto;}
#search .search-top{width: 100%; height: auto; background: #fff; padding: 10px 0;}
