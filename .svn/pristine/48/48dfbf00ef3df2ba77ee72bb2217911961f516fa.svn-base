/*滑动删除*/
.behind {width: 100%;height: auto;position: absolute;top: 0px;right: 0;}
.behind a.ui-btn {width: 100px;margin: 0px;float: right;border: none; background: #bbb; color: #fff; line-height: 25px; }
.behind a.delete-btn, .behind a.delete-btn:active, .behind a.delete-btn:visited, .behind a.delete-btn:focus, .behind a.delete-btn:hover {color: white;background-color: #bbb;text-shadow: none;}
.behind a.ui-btn.pull-left {float: left;}
.behind a.edit-btn, .behind a.edit-btn:active, .behind a.edit-btn:visited, .behind a.edit-btn:focus, .behind a.edit-btn:hover {color: white;background-color: orange;text-shadow: none;}
.swipe-delete li{margin-top: 10px;}


