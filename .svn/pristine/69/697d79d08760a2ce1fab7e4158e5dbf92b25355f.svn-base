	<script>
	$(".login-btn").click(function() {
	var username = $(".login-user").val();
	var pwd = $(".login-pwd").val();
	var pp = md5(pwd);
	var regx = /^(13[0-9]{9})|(14[0-9]{9})|(17[0-9]{9})|(18[0-9]{9})|(15[0-9]{9})$/;
	var reem = /^(\w-*\.*)+@(\w-?)+(\.\w{2,})+$/;
	var patt = new RegExp('@');
	if(username == "") {
		$.toast("请输入手机号码或者邮箱", "text");
	} else {
		if(patt.test(username)) {
			if(!reem.test(username)) {
				$.toast("请输入正确的邮箱", "text");
			} else if(pwd == "") {
				$.toast("请输入6-12位数字或字母组合的密码", "text");
			} else if(pwd.length < 6) {
				$.toast("请输入6-12位数字或字母组合的密码", "text");
			} else {
				var user = username.replace("@", "");
				console.log(user);
				var options = {
					apiUrl: WebIM.config.apiURL,
					user: user,
					pwd: pp,
					appKey: WebIM.config.appkey
				};
				conn.open(options);
				console.log(123456789);
				console.log(options);
			}
		} else {
			if(username.length < 11 || !regx.test(username) || username.length > 11) {
				$.toast("请输入正确的手机号码", "text");
			} else if(pwd == "") {
				$.toast("请输入6-12位数字或字母组合的密码", "text");
			} else if(pwd.length < 6) {
				$.toast("请输入6-12位数字或字母组合的密码", "text");
			} else {
				var options = {
					apiUrl: WebIM.config.apiURL,
					user: username,
					pwd: pp,
					appKey: WebIM.config.appkey
				};
				conn.open(options);
				console.log(123456789);
				console.log(options);

			}

		}
	}
})
